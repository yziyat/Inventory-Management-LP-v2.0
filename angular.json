{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "inventory-app": {
      "projectType": "application",
      "root": "",
      "sourceRoot": ".",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/inventory-app",
            "index": "index.html",
            "browser": "index.tsx",
            "tsConfig": "tsconfig.json",
            "assets": [],
            "styles": [],
            "allowedCommonJsDependencies": [
              "xlsx",
              "d3"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "4mb",
                  "maximumError": "5mb"
                }
              ],
              "outputHashing": "all"
            }
          }
        }
      }
    }
  },
  "defaultProject": "inventory-app"
}
